@model IEnumerable<Jobs4You.Models.Client>

@{
    ViewBag.Title = "Index";
}

<table class="table">

    <tr>
        <th>
            @Html.DisplayNameFor(model => model.client_username)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.client_pass)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.client_phone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.client_stat)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.client_rating)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.client_mail)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.client_name)
        </th>
        <th></th>
    </tr>

    @foreach(var item in Model) { 
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.client_username)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.client_pass)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.client_phone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.client_stat)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.client_rating)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.client_mail)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.client_name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.clientID }) |
            @Html.ActionLink("Details", "Details", new { id = item.clientID }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.clientID })
        </td>
    </tr>
    }
</table>